<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.cms.entity.main">
	<class
		name="AcUserTeam"
		table="ac_user_team"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="team_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="TeamName"
			column="team_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="TeamCreatetime"
			column="team_createtime"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="TeamCreateuser"
			column="team_createuser"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="TeamLevel"
			column="team_level"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="TeamScore"
			column="team_score"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="TeamRank"
			column="team_rank"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="TeamUpdatetime"
			column="team_updatetime"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="TeamIscertificate"
			column="team_iscertificate"
			type="java.lang.Integer"
			not-null="false"
			length="1"
		/>
		<property
			name="TeamIsforbid"
			column="team_isforbid"
			type="integer"
			not-null="false"
			length="1"
		/>
		<property
			name="TeamApplyallow"
			column="team_applyallow"
			type="integer"
			not-null="false"
			length="1"
		/>
		<property
			name="TeamCreateusername"
			column="team_createusername"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="TeamCreateuserduty"
			column="team_createuserduty"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="TeamCreateuserimg"
			column="team_createuserimg"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="TeamSchoolid"
			column="team_schoolid"
			type="integer"
			not-null="false"
			length="11"
		/>
		<one-to-one name="AcUserTeamExt" class="AcUserTeamExt" constrained="true"/>
		<one-to-one name="AcUserTeamFlag" class="AcUserTeamFlag" constrained="true"/>
	</class>	
</hibernate-mapping>