<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.cms.entity.main">
	<class
		name="Activity"
		table="activity"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="ac_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="AcUserid"
			column="ac_userid"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="AcCreatedate"
			column="ac_createdate"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="AcState"
			column="ac_state"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="AcStatedate"
			column="ac_statedate"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="AcBegindate"
			column="ac_begindate"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="AcCategory"
			column="ac_category"
			type="integer"
			not-null="true"
			length="11"
		/>
		<property
			name="AcSignmax"
			column="ac_signmax"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="AcSignmin"
			column="ac_signmin"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="AcAddress"
			column="ac_address"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="AcTxt"
			column="ac_txt"
			type="string"
			not-null="false"
		/>
		<property
			name="AcTypeid"
			column="ac_typeid"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="AcLevelid"
			column="ac_levelid"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="AcIsrecommend"
			column="ac_isrecommend"
			type="boolean"
			not-null="false"
		/>
		<property
			name="AcIstop"
			column="ac_istop"
			type="boolean"
			not-null="false"
		/>
		<property
			name="AcSchool"
			column="ac_school"
			type="integer"
			not-null="false"
		/>
		<property
			name="AcPass"
			column="ac_pass"
			type="integer"
			not-null="false"
		/>
		<property
			name="AcIsshow"
			column="ac_isshow"
			type="integer"
			not-null="false"
		/>
		<property
			name="AcContactmethod"
			column="ac_contactmethod"
			type="string"
			not-null="false"
		/>
		<property
			name="AcDuration"
			column="ac_duration"
			type="integer"
			not-null="false"
		/>
		<property
			name="AcTeamid"
			column="ac_teamid"
			type="integer"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>